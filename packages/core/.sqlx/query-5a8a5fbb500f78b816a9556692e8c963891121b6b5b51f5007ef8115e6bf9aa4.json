  "query": "\n        UPDATE payment_intents\n        SET status = 'canceled'::payment_intent_status,\n            canceled_at = NOW(),\n            updated_at = NOW()\n        WHERE id = $1 \n          AND platform_id = $2\n          AND status = 'requires_payment'::payment_intent_status\n        RETURNING \n            id, platform_id, amount, currency, description,\n            status as \"status: PaymentIntentStatus\",\n            capture_method as \"capture_method: CaptureMethod\",\n            client_secret, agent_id, agent_name, agent_metadata,\n            payment_id, metadata, created_at, updated_at,\n            confirmed_at, canceled_at, expires_at\n        ",
